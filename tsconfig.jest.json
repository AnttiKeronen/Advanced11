{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "verbatimModuleSyntax": false,
    "types": ["jest", "node", "vite/client"],
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false

  },
  "include": ["src/**/*.ts", "src/**/*.tsx"]
}
